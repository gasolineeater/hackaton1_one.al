# Gemini API Key - Get this from Google AI Studio or Vertex AI
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_WINDOW_MS=60000
AUTH_RATE_LIMIT_MAX_REQUESTS=5
API_RATE_LIMIT_WINDOW_MS=60000
API_RATE_LIMIT_MAX_REQUESTS=50
RECOMMENDATION_RATE_LIMIT_WINDOW_MS=300000
RECOMMENDATION_RATE_LIMIT_MAX_REQUESTS=10
ADAPTIVE_RATE_LIMIT_BASE=100
ADAPTIVE_RATE_LIMIT_MIN=10

# Caching Configuration
USE_REDIS_CACHE=false
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
CACHE_TTL=60
MEMORY_CACHE_MAX_SIZE=1000
MEMORY_CACHE_CHECK_PERIOD=60000

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=one_albania_db
DB_USER=root
DB_PASSWORD=password

# Database Optimization
DB_POOL_MAX=10
DB_POOL_MIN=2
DB_POOL_IDLE=10000
DB_POOL_ACQUIRE=60000
DB_VALIDATE_INTERVAL=60000
SLOW_QUERY_THRESHOLD_MS=500

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_ACCESS_EXPIRATION=15m
JWT_REFRESH_EXPIRATION=7d
